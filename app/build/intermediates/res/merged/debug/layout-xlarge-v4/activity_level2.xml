<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.pc.projectdrag.level2"
    android:background="@drawable/level1background">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

        <Button
            android:id="@+id/menu"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:layout_marginBottom="8dp"
            android:background="@drawable/menu"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="@+id/reset"
            android:layout_marginTop="0dp"
            app:layout_constraintVertical_bias="0.0"
            android:layout_marginLeft="95dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <Button
            android:id="@+id/reset"
            android:layout_width="97dp"
            android:layout_height="97dp"
            android:background="@drawable/reset"
            android:onClick="resetBtn"
            android:text=""
            tools:layout_editor_absoluteY="1090dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/menu"
            android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.657" />

        <ImageView
            android:id="@+id/panneau"
            android:layout_width="159dp"
            android:layout_height="509dp"
            android:layout_marginBottom="206dp"
            android:layout_marginEnd="20dp"
            android:layout_marginRight="54dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/c1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            app:layout_constraintLeft_toRightOf="@+id/icon2"
            android:layout_marginLeft="8dp"
            app:layout_constraintHorizontal_bias="0.679"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="1.0" />

        <ImageView
            android:id="@+id/spin4"
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinv"
            tools:layout_editor_absoluteX="117dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageView
            android:id="@+id/spin3"
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinv"
            tools:layout_editor_absoluteX="117dp"
            tools:layout_editor_absoluteY="56dp" />

        <ImageView
            android:id="@+id/spin2"
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinv"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageView
            android:id="@+id/spin1"
            android:layout_width="130dp"
            android:layout_height="40dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinv"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="56dp" />

        <ImageView
            android:id="@+id/icon1"
            android:layout_width="200dp"
            android:layout_height="120dp"
            android:layout_marginLeft="26dp"
            android:layout_marginRight="8dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconv"
            app:layout_constraintHorizontal_bias="0.037"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_editor_absoluteY="877dp" />

        <ImageView
            android:id="@+id/icon2"
            android:layout_width="200dp"
            android:layout_height="120dp"
            android:layout_marginLeft="26dp"
            android:layout_marginRight="8dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconv"
            app:layout_constraintHorizontal_bias="0.037"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginBottom="-58dp"
            app:layout_constraintBottom_toTopOf="@+id/icon1" />

        <ImageView
            android:id="@+id/icon3"
            android:layout_width="200dp"
            android:layout_height="120dp"
            android:layout_marginLeft="26dp"
            android:layout_marginRight="8dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconv"
            app:layout_constraintHorizontal_bias="0.037"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginBottom="-11dp"
            app:layout_constraintBottom_toTopOf="@+id/icon2" />

        <ImageView
            android:id="@+id/icon4"
            android:layout_width="200dp"
            android:layout_height="120dp"
            android:layout_marginLeft="26dp"
            android:layout_marginRight="8dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconv"
            app:layout_constraintHorizontal_bias="0.037"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginBottom="-35dp"
            app:layout_constraintBottom_toTopOf="@+id/icon3" />


    </android.support.constraint.ConstraintLayout>

    <Chronometer
        android:id="@+id/chrono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="38dp" />

</android.support.constraint.ConstraintLayout>
