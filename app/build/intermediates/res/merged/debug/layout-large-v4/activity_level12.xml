<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl11"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.pc.projectdrag.level1"
    android:background="@drawable/background">


    <android.support.constraint.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

        <Button
            android:id="@+id/reset"
            android:layout_width="58dp"
            android:layout_height="52dp"
            android:layout_marginBottom="8dp"
            android:layout_marginLeft="89dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:background="@drawable/reset"
            android:onClick="resetBtn"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.576"
            app:layout_constraintLeft_toRightOf="@+id/menu"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            tools:layout_editor_absoluteY="887dp" />

        <Button
            android:id="@+id/menu"
            android:layout_width="64dp"
            android:layout_height="54dp"
            android:layout_marginBottom="6dp"
            android:background="@drawable/menu"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="86dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <ImageView
            android:id="@+id/iconn"
            android:layout_width="116dp"
            android:layout_height="69dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="312dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconvi"
            app:layout_constraintHorizontal_bias="0.016"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/iconv"
            android:layout_width="116dp"
            android:layout_height="70dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="172dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconb"
            app:layout_constraintHorizontal_bias="0.017"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/iconr"
            android:layout_width="116dp"
            android:layout_height="70dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="242dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconbr"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/image1"
            android:layout_width="95dp"
            android:layout_height="370dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/c11"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="62dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.885"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintHorizontal_bias="0.929" />

        <ImageView
            android:id="@+id/image2"
            android:layout_width="63dp"
            android:layout_height="136dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/c22"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="53dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.91"
            app:layout_constraintHorizontal_bias="0.47" />

        <ImageView
            android:id="@+id/image3"
            android:layout_width="77dp"
            android:layout_height="217dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/c33"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_marginBottom="8dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.045"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.857"
            app:layout_constraintBottom_toBottomOf="parent" />


        <ImageView
            android:id="@+id/spin"
            android:layout_width="138dp"
            android:layout_height="46dp"
            android:layout_marginLeft="224dp"
            android:layout_marginTop="121dp"
            android:visibility="invisible"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/spinb"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1" />

        <ImageView
            android:id="@+id/spinR"
            android:layout_width="138dp"
            android:layout_height="46dp"
            android:layout_marginLeft="217dp"
            android:layout_marginTop="126dp"
            android:visibility="invisible"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/spinbr"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1" />

        <ImageView
            android:id="@+id/spinV"
            android:layout_width="138dp"
            android:layout_height="46dp"
            android:layout_marginStart="248dp"
            android:layout_marginTop="126dp"
            android:visibility="invisible"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/spinvi"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            tools:layout_editor_absoluteX="224dp" />

        <Chronometer
            android:id="@+id/chrono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            tools:layout_constraintTop_creator="1"
            android:layout_marginStart="7dp"
            android:layout_marginTop="1dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="7dp" />

    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>

