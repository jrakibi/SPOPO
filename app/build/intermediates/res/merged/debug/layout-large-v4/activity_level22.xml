<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl22"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.pc.projectdrag.level2"
    android:background="@drawable/level1background">

    <android.support.constraint.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

        <Button
            android:id="@+id/menu"
            android:layout_width="62dp"
            android:layout_height="66dp"
            android:layout_marginBottom="16dp"
            android:background="@drawable/menu"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="63dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <Button
            android:id="@+id/reset"
            android:layout_width="68dp"
            android:layout_height="68dp"
            android:text=""
            android:background="@drawable/reset"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="16dp"
            android:onClick="resetBtn"
            app:layout_constraintLeft_toRightOf="@+id/menu"
            android:layout_marginLeft="113dp"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.722" />

        <ImageView
            android:id="@+id/panneau"
            android:layout_width="98dp"
            android:layout_height="381dp"
            android:layout_marginBottom="50dp"
            android:layout_marginEnd="20dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="16dp"
            android:layout_marginTop="8dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/c11"
            app:layout_constraintBottom_toTopOf="@+id/reset"
            app:layout_constraintHorizontal_bias="0.508"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.943"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1" />

        <ImageView
            android:id="@+id/spin4"
            android:layout_width="134dp"
            android:layout_height="46dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinb"
            tools:layout_editor_absoluteX="403dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageView
            android:id="@+id/spin3"
            android:layout_width="134dp"
            android:layout_height="46dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinb"
            tools:layout_editor_absoluteX="403dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageView
            android:id="@+id/spin2"
            android:layout_width="134dp"
            android:layout_height="46dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinb"
            tools:layout_editor_absoluteX="403dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageView
            android:id="@+id/spin1"
            android:layout_width="134dp"
            android:layout_height="46dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinb"
            tools:layout_editor_absoluteX="403dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageView
            android:id="@+id/icon1"
            android:layout_width="116dp"
            android:layout_height="75dp"
            android:layout_marginLeft="480dp"
            android:layout_marginRight="480dp"
            android:layout_marginTop="239dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconvi"
            app:layout_constraintHorizontal_bias="0.056"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1" />

        <ImageView
            android:id="@+id/icon2"
            android:layout_width="116dp"
            android:layout_height="75dp"
            android:layout_marginLeft="480dp"
            android:layout_marginRight="480dp"
            android:layout_marginTop="314dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconvi"
            app:layout_constraintHorizontal_bias="0.058"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1" />

        <ImageView
            android:id="@+id/icon3"
            android:layout_width="116dp"
            android:layout_height="75dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="247dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconvi"
            app:layout_constraintHorizontal_bias="0.073"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1" />

        <ImageView
            android:id="@+id/icon4"
            android:layout_width="116dp"
            android:layout_height="75dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="322dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconvi"
            app:layout_constraintHorizontal_bias="0.073"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1" />


    </android.support.constraint.ConstraintLayout>

    <Chronometer
        android:id="@+id/chrono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

</android.support.constraint.ConstraintLayout>
