<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rl2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.pc.projectdrag.level1"
    android:background="@drawable/background">


    <android.support.constraint.ConstraintLayout
        android:id="@+id/main"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1">

        <Button
            android:id="@+id/menu"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginBottom="0dp"
            android:background="@drawable/menu"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="37dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <Button
            android:id="@+id/reset"
            android:layout_width="12dp"
            android:layout_height="12dp"
            android:layout_marginBottom="0dp"
            android:layout_marginLeft="45dp"
            android:background="@drawable/reset"
            android:onClick="resetBtn"
            android:text=""
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toRightOf="@+id/menu"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.347" />

        <ImageView
            android:id="@+id/iconv"
            android:layout_width="44dp"
            android:layout_height="28dp"
            android:layout_marginTop="1dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconv"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginLeft="5dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <ImageView
            android:id="@+id/iconj"
            android:layout_width="44dp"
            android:layout_height="28dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconj"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="130dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <ImageView
            android:id="@+id/iconr"
            android:layout_width="44dp"
            android:layout_height="28dp"
            android:layout_marginTop="-8dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/iconr"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginLeft="69dp"
            app:layout_constraintLeft_toLeftOf="parent" />

        <ImageView
            android:id="@+id/image1"
            android:layout_width="26dp"
            android:layout_height="94dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/c1"
            android:layout_marginEnd="48dp"
            android:layout_marginRight="50dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.89" />

        <ImageView
            android:id="@+id/image2"
            android:layout_width="24dp"
            android:layout_height="68dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/c2"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginLeft="49dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="1.0"
            android:layout_marginRight="8dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintHorizontal_bias="0.117" />

        <ImageView
            android:id="@+id/image3"
            android:layout_width="19dp"
            android:layout_height="41dp"
            android:contentDescription="@string/app_name"
            android:src="@drawable/c3"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintRight_toLeftOf="@+id/image2"
            android:layout_marginRight="8dp"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp"
            app:layout_constraintHorizontal_bias="0.181"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="8dp"
            app:layout_constraintVertical_bias="0.936" />


        <ImageView
            android:id="@+id/spin"
            android:layout_width="43dp"
            android:layout_height="14dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinj"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="29dp"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toRightOf="parent" />

        <ImageView
            android:id="@+id/spinR"
            android:layout_width="43dp"
            android:layout_height="14dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinr"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="43dp" />

        <ImageView
            android:id="@+id/spinV"
            android:layout_width="43dp"
            android:layout_height="14dp"
            android:visibility="invisible"
            app:srcCompat="@drawable/spinv"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="57dp"
            android:layout_marginRight="5dp"
            app:layout_constraintRight_toRightOf="parent" />

        <Chronometer
            android:id="@+id/chrono"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="invisible"
            tools:layout_constraintTop_creator="1"
            tools:layout_constraintBottom_creator="1"
            android:layout_marginStart="7dp"
            android:layout_marginTop="325dp"
            tools:layout_constraintLeft_creator="1"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginLeft="7dp" />

    </android.support.constraint.ConstraintLayout>

</android.support.constraint.ConstraintLayout>

